    "@types/node": "^11.11.0",
    "@types/tmp": "0.0.34",
    "@types/uuid": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
