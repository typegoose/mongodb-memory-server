{
  "name": "mongodb-memory-server-core",
  "version": "11.0.1",
  "description": "MongoDB Server for testing (core package, without autodownload). The server will allow you to connect your favourite ODM or client library to the MongoDB Server and run parallel integration tests isolated from each other.",
  "main": "lib/index",
  "types": "lib/index.d.ts",
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/typegoose/mongodb-memory-server.git",
    "directory": "packages/mongodb-memory-server-core"
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "files": [
    "lib",
    "scripts"
  ],
  "keywords": [
    "mongodb",
    "mongoose",
    "mock",
    "stub",
    "mockgoose",
    "mongodb-prebuilt",
    "mongomem"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/typegoose/mongodb-memory-server/issues"
  },
  "homepage": "https://github.com/typegoose/mongodb-memory-server",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/find-cache-dir": "^3.2.1",
    "@types/follow-redirects": "^1.14.4",
    "@types/semver": "^7.7.1",
    "@types/tar-stream": "^3.1.4",
    "@types/yauzl": "^2.10.3",
    "@types/yazl": "^3.3.0",
    "rimraf": "^6.1.2",
    "yazl": "^3.3.1"
  },
  "dependencies": {
    "async-mutex": "^0.5.0",
    "camelcase": "^6.3.0",
    "debug": "^4.4.3",
    "find-cache-dir": "^3.3.2",
    "follow-redirects": "^1.15.11",
    "https-proxy-agent": "^7.0.6",
    "mongodb": "^7.0.0",
    "new-find-package-json": "^2.0.0",
    "semver": "^7.7.3",
    "tar-stream": "^3.1.7",
    "tslib": "^2.8.1",
    "yauzl": "^3.2.0"
  },
  "scripts": {
    "clean": "rimraf tmp lib build coverage node_modules/.cache",
    "build": "rimraf ./lib && tsc -p ./tsconfig.build.json",
    "build:tests": "rimraf ./build && tsc -p ./tsconfig.json",
    "coverage": "jest --coverage",
    "lint": "eslint -c ../../eslint.config.mjs .",
    "test": "npm run lint && npm run coverage",
    "test:watch": "jest --watchAll",
    "jest": "jest"
  }
}
