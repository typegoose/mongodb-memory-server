{
  "version": "6.5.1",
  "command": {
    "publish": {
      "allowBranch": [
        "master",
        "fix-lerna"
      ],
      "exact": true,
      "conventionalCommits": true,
      "ignoreChanges": [
        "**/__fixtures__/**",
        "**/__tests__/**",
        "**/*.md"
      ]
    },
    "version": {
      "createRelease": "github"
    }
  }
}
